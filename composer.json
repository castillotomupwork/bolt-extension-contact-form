{
    "name": "tomcastillo/contactform",
    "description": "Contact Form",
    "type": "bolt-extension",
    "keywords": ["contact", "form"],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Castillo",
            "email": "castillo.tom.upwork@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\TomCastillo\\ContactForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\TomCastillo\\ContactForm\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\TomCastillo\\ContactForm\\ContactFormExtension"
    }
}
